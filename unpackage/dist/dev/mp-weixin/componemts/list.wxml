<view class="constormList"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="ind" wx:key="ind"><view><block wx:if="{{item.url}}"><navigator class="listLine" url="{{item.url}}"><view class="listname"><block wx:if="{{item.icon}}"><image src="{{item.icon}}" mode="widthFix"></image></block><text>{{item.name}}</text></view><view class="submsg"><block wx:if="{{item.subImg}}"><view class="subImg"><image src="{{item.subImg}}" mode="widthFix"></image></view></block><block wx:if="{{item.subText}}"><text>{{item.subText}}</text></block><image src="../static/image/rightArrow.png" mode="widthFix"></image></view></navigator></block><block wx:else><block wx:if="{{item.method}}"><view data-event-opts="{{[['tap',[['editMsg',['$event']]]]]}}" class="listLine" bindtap="__e"><view class="listname"><block wx:if="{{item.icon}}"><image src="{{item.icon}}" mode="widthFix"></image></block><text>{{item.name}}</text></view><view class="submsg"><block wx:if="{{item.subImg}}"><view class="subImg"><image src="{{item.subImg}}" mode="widthFix"></image></view></block><block wx:if="{{item.subText}}"><text>{{item.subText}}</text></block><image src="../static/image/rightArrow.png" mode="widthFix"></image></view></view></block><block wx:else><view class="showmsg listLine"><view class="listname"><block wx:if="{{item.icon}}"><image src="{{item.icon}}" mode="widthFix"></image></block><text>{{item.name}}</text></view><block wx:if="{{item.subText}}"><view class="submsg"><text>{{item.subText}}</text></view></block></view></block></block></view></block></view>